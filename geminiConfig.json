{
  "mcpServers": {
    "jadx-mcp": {
      "command": "uv",
      "args": [
        "--directory",
        "/MCPs/jadx-mcp-server",
        "run",
        "jadx_mcp_server.py"
      ],
      "cwd": "/MCPs/jadx-mcp-server",
      "timeout": 30000,
      "trust": true
    },
    "ghidra-mcp": {
      "command": "python3",
      "args": [
        "/MCPs/GhidraMCP/bridge_mcp_ghidra.py",
        "--ghidra-server",
        "http://127.0.0.1:8080/"
      ]
    }
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  }
}