{
  "mcpServers": {
    "jadx-mcp": {
      "command": "python3",
      "args": [
        "/workspace/MCP_servers/jadx-mcp-server/jadx_mcp_server.py"
        // "run", "--with", "xmltodict",
      ],
      "cwd": "/workspace/MCP_servers/jadx-mcp-server",
      "timeout": 30000,
      "trust": true
    },
    "ghidra-mcp": {
      "command": "python3",
      "args": [
        "/workspace/MCP_servers/GhidraMCP/bridge_mcp_ghidra.py",
        "--ghidra-server",
        "http://127.0.0.1:8080/"
      ]
    }
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  }
}